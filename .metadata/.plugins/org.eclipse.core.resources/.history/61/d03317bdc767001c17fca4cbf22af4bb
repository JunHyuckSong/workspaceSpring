				<prop key="/main.do"></prop>
				<prop key="/board.do"></prop>
				<prop key="/logout.do"></prop>
				<prop key="/insertBoard.do"></prop>
				<prop key="/deleteBoard.do"></prop>
				<prop key="/updateBoard.do"></prop>
			</props>
		</property>
	</bean>
	
	
